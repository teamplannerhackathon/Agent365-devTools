  {
    "name": "mcp_MailTools_graph_mail_searchMessages",
    "description": "Search Outlook messages using Microsoft Graph Search API (/v1.0/search/query) with entityTypes=[\u0022message\u0022].",
    "inputSchema": {
      "type": "object",
      "properties": {
        "requests": {
          "type": "array",
          "description": "Array of searchRequest objects; typically send one.",
          "x-ms-location": "body",
          "x-ms-path": "requests",
          "items": {
            "type": "object",
            "properties": {
              "entityTypes": {
                "type": "array",
                "description": "Search entity types. For messages use [\u0022message\u0022].",
                "items": {
                  "type": "string"
                },
                "default": [
                  "message"
                ]
              },
              "query": {
                "type": "object",
                "description": "KQL-style search string for messages (subject, body, attachments).",
                "properties": {
                  "queryString": {
                    "type": "string",
                    "description": "The search text, e.g., \u0022contoso OR from:deepthi\u0022"
                  }
                },
                "required": [
                  "queryString"
                ]
              },
              "from": {
                "type": "integer",
                "description": "Zero-based offset of the first result to return.",
                "minimum": 0,
                "default": 0
              },
              "size": {
                "type": "integer",
                "description": "Page size (results to return).",
                "minimum": 1,
                "maximum": 50,
                "default": 25
              },
              "enableTopResults": {
                "type": "boolean",
                "description": "If true, returns Top results (relevance booster).",
                "default": false
              }
            },
            "required": [
              "entityTypes",
              "query"
            ]
          }
        }
      },
      "required": [
        "requests"
      ]
    },
    "responseTemplate": "Search completed for messages (mock).",
    "delayMs": 250,
    "errorRate": 0,
    "statusCode": 200,
    "enabled": true
  },
